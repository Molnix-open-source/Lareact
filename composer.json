{
    "name": "fizzybit/lareact",
    "description": "Scafold react with react router in laravel app",
    "keywords": [
        "fizzybit",
        "lareact"
    ],
    "homepage": "https://github.com/fizzybit/lareact",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vish",
            "email": "vish@fizzybit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Fizzybit\\Lareact\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fizzybit\\Lareact\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fizzybit\\Lareact\\LareactServiceProvider"
            ],
            "aliases": {
                "Lareact": "Fizzybit\\Lareact\\LareactFacade"
            }
        }
    }
}
