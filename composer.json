{
    "name": "molnix/lareact",
    "description": "Scafold react with react router in laravel app",
    "keywords": [
        "molnix",
        "lareact"
    ],
    "homepage": "https://github.com/molnix/lareact",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vish",
            "email": "vishnu@molnix.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/validation": "^11.0|^12.0",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "laravel/framework": "^11.0|^12.0",
        "orchestra/testbench": "^10.1",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Molnix\\Lareact\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Molnix\\Lareact\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Molnix\\Lareact\\LareactServiceProvider"
            ]
        }
    }
}